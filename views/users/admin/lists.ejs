<% layout("layouts/boilerplate") %>
          

            <!-- Listings Table -->
            <div class="card">
                <div class="card-header">
                    <h5>Listings</h5>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Title</th>
                                    <th>Owner</th>
                                    <th>Price</th>
                                    <th>Location</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% listings.forEach(listing => { %>
                                <tr>
                                    <td><%= listing.title %></td>
                                    <td>
                                        <% if(listing.owner) { %>
                                            <%= listing.owner.username %>
                                        <% } else { %>
                                            N/A
                                        <% } %>
                                    </td>
                                    <td>â‚¹<%= listing.price %></td>
                                    <td><%= listing.location %></td>
                                </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>